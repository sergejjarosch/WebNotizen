@model IEnumerable<Notiz>

@if (Model != null && Model.Count() > 0)
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Notiz Title</th>
                <th>Datum : Zeit</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var notiz in Model)
            {
                <tr class="product-row" product-id="@notiz.NotizId">
                    <td>@notiz.Title</td>
                    <td>@notiz.TimeStamp</td>
                </tr>
            }
        </tbody>
    </table>
}
